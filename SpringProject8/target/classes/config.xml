<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<context:annotation-config />
	<bean class="com.spring.autowiringusingAnnotationAndQualifiers.SpringProject8.Car"
		id="car">
	</bean>
	<bean
		class="com.spring.autowiringusingAnnotationAndQualifiers.SpringProject8.Driver">
		<property name="id" value="1"></property>
		<property name="name" value="Ankit"></property>
		<property name="age" value="22"></property>
	</bean>
<bean class="com.spring.autowiringusingAnnotationAndQualifiers.SpringProject8.Driver"
		id="d1">
		<property name="id" value="2"></property>
		<property name="name" value="Ajay"></property>
		<property name="age" value="25"></property>
	</bean>
	<bean class="com.spring.autowiringusingAnnotationAndQualifiers.SpringProject8.Driver"
		id="d2">
		<property name="id" value="3"></property>
		<property name="name" value="Arun"></property>
		<property name="age" value="27"></property>
	</bean>
	<bean
		class="com.spring.autowiringusingAnnotationAndQualifiers.SpringProject8.University"
		id="uni">
	</bean>
	<bean
		class="com.spring.autowiringusingAnnotationAndQualifiers.SpringProject8.Professor">
		<property name="p_id" value="11"></property>
		<property name="name" value="Praveen"></property>
		<property name="subject" value="Math"></property>
	</bean>
	<bean class="com.spring.autowiringusingAnnotationAndQualifiers.SpringProject8.Professor"
		id="p2">
		<property name="p_id" value="22"></property>
		<property name="name" value="Pratik"></property>
		<property name="subject" value="Science"></property>
	</bean>
</beans>