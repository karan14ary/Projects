<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<context:annotation-config />
	<bean class="com.spring.autowiringusingAnnotation.SpringProject7.Car"
		id="car">
	</bean>
	<bean
		class="com.spring.autowiringusingAnnotation.SpringProject7.Driver">
		<property name="id" value="1"></property>
		<property name="name" value="Ankit"></property>
		<property name="age" value="22"></property>
	</bean>

	<bean
		class="com.spring.autowiringusingAnnotation.SpringProject7.University"
		id="uni">
	</bean>
	<bean
		class="com.spring.autowiringusingAnnotation.SpringProject7.Professor">
		<property name="p_id" value="11"></property>
		<property name="name" value="Praveen"></property>
		<property name="subject" value="Math"></property>
	</bean>

</beans>